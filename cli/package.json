{
  "name": "textusm.cli",
  "description": "Generate a User Story Mapping from indented text.",
  "version": "0.14.5",
  "author": "Takahiro Sato",
  "bin": {
    "textusm": "dist/index.js"
  },
  "bugs": {
    "url": "https://github.com/harehare/textusm/issues"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "html-escaper": "^3.0.3",
    "mustache": "^4.2.0",
    "puppeteer": "^21.10.0",
    "svgo": "^3.2.0"
  },
  "devDependencies": {
    "@types/html-escaper": "^3.0.2",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.13",
    "@types/puppeteer": "^5.4.7",
    "@types/svgo": "^2.6.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "homepage": "https://github.com/harehare/textusm",
  "keywords": [
    "cli",
    "user-story-map",
    "user-story-mapping",
    "usm",
    "svg",
    "png",
    "pdf"
  ],
  "files": [
    "dist",
    "js"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harehare/textusm.git"
  },
  "scripts": {
    "build": "tsup index.ts js/textusm.js",
    "prepublish": "tsc index.ts --outDir dist",
    "prebuild": "cd ../extension/lib && npm run prod && cp dist/index.js ../../cli/js/textusm.js"
  },
  "tsup": {
    "entry": [
      "index.ts",
      "js/textusm.js"
    ],
    "splitting": false,
    "clean": true
  }
}
