icon : String -> Int -> Int -> String -> List (Svg msg) -> Svg msg
icon color w h v children =
    Svg.svg
        [ width <| String.fromInt w
        , height <| String.fromInt h
        , viewBox v
        ]
        [ Svg.g
            [ fill color ]
            children
        ]
