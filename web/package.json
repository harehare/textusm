{
  "name": "textusm",
  "description": "Online tool for Generate a User Story Mapping from indented text.",
  "version": "0.12.1",
  "author": "harehare",
  "bugs": {
    "url": "https://github.com/harehare/textusm/issues"
  },
  "dependencies": {
    "@sentry/browser": "^7.52.1",
    "clipboard-copy": "^4.0.1",
    "dexie": "^3.2.3",
    "firebase": "^9.22.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "monaco-editor": "^0.38.0",
    "svgo": "^3.0.2",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@dillonkearns/elm-graphql": "^4.3.1",
    "@sentry/webpack-plugin": "^1.20.1",
    "@size-limit/file": "^8.2.4",
    "@total-typescript/ts-reset": "^0.4.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/strictest": "^2.0.1",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20.1.4",
    "@types/requestidlecallback": "^0.3.5",
    "@types/uuid": "^9.0.1",
    "@types/wicg-file-system-access": "^2020.9.6",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "cssnano-preset-advanced": "^6.0.1",
    "cypress": "^12.12.0",
    "devcert": "^1.2.2",
    "elm": "^0.19.1-5",
    "elm-book": "^1.0.1",
    "elm-constants": "^1.0.0",
    "elm-format": "^0.8.7",
    "elm-hot-webpack-loader": "^1.1.8",
    "elm-optimize-level-2": "^0.3.5",
    "elm-review": "^2.10.2",
    "elm-test": "^0.19.1-revision12",
    "elm-webpack-loader": "^8.0.0",
    "firebase-tools": "^12.0.0",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.5",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "size-limit": "^8.0.1",
    "style-loader": "^3.3.2",
    "stylelint": "^15.6.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser": "^5.17.3",
    "terser-webpack-plugin": "^5.3.8",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.8.0",
    "workbox-sw": "^6.5.4",
    "workbox-webpack-plugin": "^6.5.4",
    "xo": "^0.54.2"
  },
  "homepage": "https://github.com/harehare/textusm",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harehare/textusm.git"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:web": "webpack serve --hot --color",
    "dev:server": "cd ../server/ && just watch",
    "dev:emulators": "cd .. && firebase emulators:start --only firestore,auth,storage",
    "setup:emulators": "cd .. && firebase init emulators",
    "dev:generate-elm-constants": "npm run generate-elm-constants",
    "prod": "NODE_ENV=production webpack --mode production",
    "prod:run": "NODE_ENV=production webpack serve --color --mode production",
    "postprod": "rimraf -rf dist/*.map",
    "embed:build": "npm run prod && cp -r dist ../server/pkg/app",
    "start": "npm run dev",
    "start:web": "webpack serve --no-hot --no-client-overlay --port 3000",
    "test": "elm-test",
    "test:e2e": "cypress run",
    "open:e2e": "cypress open",
    "vscode:build": "elm make src/elm/Extension/VSCode.elm --optimize --output elm.optimized.js && terser elm.optimized.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters,keep_fargs=false,unsafe_comps,unsafe' --mangle --output=elm.js && rimraf elm.optimized.js && mv elm.js ../extension/vscode/js/",
    "lib:build": "elm make src/elm/Extension/Lib.elm --optimize --output elm.optimized.js && terser elm.optimized.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters,keep_fargs=false,unsafe_comps,unsafe' --mangle --output=elm.js && rimraf elm.optimized.js && mv elm.js ../extension/lib/src/js/",
    "generate-elm-types": "elm-graphql http://localhost:8081/graphql --base Graphql --output src/Graphql",
    "generate-elm-constants": "elm-constants --no-dotenv",
    "format:ts": "prettier --write src/**/*.ts",
    "format:elm": "elm-format src/elm/**/*.elm",
    "format:validate": "concurrently \"npm:format:validate:*\"",
    "format:validate:ts": "prettier --check src/**/*.ts",
    "format:validate:elm": "elm-format --validate src/elm/**/*.elm",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:ts": "xo --prettier src/ts/*.ts",
    "lint:elm": "elm-review",
    "lint:elm:suppress": "elm-review suppress",
    "lint-fix": "concurrently \"npm:lint-fix:*\"",
    "lint-fix:ts": "xo --fix --prettier src/ts/*.ts",
    "lint-fix:elm": "elm-review --fix",
    "size": "npm run prod && size-limit",
    "book": "mkdir -p ./src/dist && cp ./src/book.html ./src/dist/ && cp -r ./src/assets/images ./src/dist/ && elm-book src/elm/Views/Book.elm --dir=./src/dist --start-page=book.html --pushstate --port 4000  -- --output=./src/dist/book.js",
    "knip": "knip"
  },
  "browserslist": [
    "defaults"
  ],
  "packageManager": "npm@9.2.0",
  "xo": {
    "semi": false,
    "semicolon": true,
    "prettier": true,
    "space": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "ignores": [
      "webpack.config.js",
      "e2e/**"
    ],
    "rules": {
      "import/extensions": "off",
      "import/order": "off",
      "n/prefer-global/process": "off",
      "@typescript-eslint/naming-convention": "off",
      "accessor-pairs": "off",
      "n/file-extension-in-import": "off",
      "unicorn/prefer-module": "off",
      "@typescript-eslint/consistent-type-definitions": "warn",
      "@typescript-eslint/no-unsafe-return": "warn",
      "@typescript-eslint/no-unsafe-call": "warn",
      "@typescript-eslint/no-unsafe-assignment": "warn"
    }
  },
  "size-limit": [
    {
      "path": "dist/*.js"
    }
  ]
}
