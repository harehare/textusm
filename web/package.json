        "node-sass": "^6.0.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.3.6",
        "postcss-loader": "^6.1.1",
        "preload-webpack-plugin": "^3.0.0-beta.4",
        "prettier": "^2.4.0",
        "sass": "^1.39.2",
        "sass-loader": "^12.1.0",
        "style-loader": "^3.2.1",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
