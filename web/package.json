{
    "name": "textusm",
    "description": "Online tool for Generate a User Story Mapping from indented text.",
    "version": "0.4.2",
    "author": "harehare",
    "bugs": {
        "url": "https://github.com/harehare/textusm/issues"
    },
    "dependencies": {
        "dexie": "^3.0.1",
        "firebase": "^7.14.3",
        "html2canvas": "^1.0.0-rc.3",
        "jspdf": "^1.5.3",
        "lzutf8": "^0.5.5",
        "monaco-editor": "^0.20.0",
        "uuid": "^8.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.9.6",
        "@dillonkearns/elm-graphql": "^4.0.2",
        "@types/jspdf": "^1.3.3",
        "@types/node": "^13.13.5",
        "@types/uuid": "^7.0.3",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "closure-webpack-plugin": "^2.3.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.5.3",
        "cssnano": "^4.1.10",
        "cssnano-preset-advanced": "^4.0.7",
        "elm": "^0.19.1-3",
        "elm-hot-webpack-loader": "^1.1.6",
        "elm-test": "^0.19.1",
        "elm-verify-examples": "^5.0.0",
        "elm-webpack-loader": "^6.0.1",
        "file-loader": "^6.0.0",
        "google-closure-compiler": "^20200504.0.0",
        "html-inline-css-webpack-plugin": "^1.8.0",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.9.0",
        "monaco-editor-webpack-plugin": "^1.9.0",
        "node-sass": "^4.14.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "preload-webpack-plugin": "^3.0.0-beta.4",
        "resolve-url-loader": "^3.1.1",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "svg-inline-loader": "^0.8.2",
        "terser-webpack-plugin": "^3.0.1",
        "ts-loader": "^7.0.3",
        "typescript": "^3.8.3",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2",
        "workbox-sw": "^5.1.3",
        "workbox-webpack-plugin": "^5.1.3"
    },
    "homepage": "https://github.com/harehare/textusm",
    "license": "MIT",
    "prettier": {
        "tabWidth": 4
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/harehare/textusm.git"
    },
    "scripts": {
        "build": "webpack",
        "deploy": "NODE_ENV=production webpack --mode production && cp -r ../.well-known dist/ && firebase deploy --only hosting:textusm",
        "dev": "webpack-dev-server --hot --color --port 3000",
        "prod": "NODE_ENV=production webpack --mode production",
        "prod:run": "NODE_ENV=production webpack-dev-server --color --port 3000 --mode production",
        "start": "npm run dev",
        "test": "elm-test",
        "vscode:build": "elm make src/elm/Extension/VSCode.elm --optimize --output elm.optimized.js && google-closure-compiler --js=elm.optimized.js --js_output_file=elm.js && rimraf elm.optimized.js && mv elm.js ../extension/vscode/js/",
        "lib:build": "elm make src/elm/Extension/Lib.elm --optimize --output elm.optimized.js && google-closure-compiler --js=elm.optimized.js --js_output_file=elm.js && rimraf elm.optimized.js && mv elm.js ../extension/lib/src/js/",
        "generate-elm-types": "elm-graphql http://localhost:8081/graphql --base TextUSM --output src/elm"
    }
}
