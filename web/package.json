        "@trivago/prettier-plugin-sort-imports": "^2.0.4",
        "@types/jspdf": "^2.0.0",
        "@types/node": "^16.9.1",
        "@types/requestidlecallback": "^0.3.4",
        "@types/svgo": "^2.4.1",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^9.0.1",
